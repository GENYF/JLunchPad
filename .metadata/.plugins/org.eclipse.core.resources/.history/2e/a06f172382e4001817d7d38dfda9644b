package List;

public class Phone_info{
	String name;
	String telnum;
	String address;
	
	public void insert(String name, String telnum, String address) {
		this.name = name;
		this.telnum = telnum;
		this.address = address;
	}
	
	public void print() {
		System.out.println("------------------------------");
		System.out.println("이     름 : " + this.name);
		System.out.println("전화번호 : " + this.telnum);
		System.out.println("주     소 : " + this.address);
	}
}